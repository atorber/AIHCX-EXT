{
  "name": "aihc-helper-extension-react",
  "version": "0.8.0",
  "description": "AIHC助手浏览器扩展",
  "type": "module",
  "scripts": {
    "dev": "vite build --watch --mode development",
    "dev:hot": "node scripts/chrome-hot-reload.js",
    "dev:hot-advanced": "node scripts/chrome-hot-reload-advanced.js",
    "build": "tsc && vite build --mode production && npm run generate-icons",
    "preview": "vite preview",
    "generate-icons": "node scripts/generate-icons.js",
    "type-check": "tsc --noEmit",
    "package": "node scripts/package-extension.js",
    "test-release": "node scripts/test-release.js",
    "release": "npm run test-release"
  },
  "dependencies": {
    "@ant-design/icons": "^6.0.2",
    "@baiducloud/sdk": "^1.0.3",
    "@types/request": "^2.48.12",
    "@types/request-promise": "^4.1.51",
    "antd": "^5.27.4",
    "axios": "^1.8.4",
    "js-yaml": "^4.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "request": "^2.88.2",
    "request-promise": "^4.2.6"
  },
  "devDependencies": {
    "@types/chrome": "^0.0.313",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.14.0",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^4.0.3",
    "less": "^4.4.1",
    "sharp": "^0.33.2",
    "typescript": "~5.7.2",
    "vite": "^6.2.0",
    "web-ext": "^8.10.0"
  }
}
